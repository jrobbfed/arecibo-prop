	Eprint = {astro-ph/0505042},
	Journal = {\mnras},
	Keywords = {galaxies: elliptical and lenticular, cD, galaxies: evolution, galaxies: formation, galaxies: kinematics and dynamics, galaxies: structure},
	Month = mar,
	Pages = {1126-1150},
	Title = {{The SAURON project - IV. The mass-to-light ratio, the virial mass estimator and the Fundamental Plane of elliptical and lenticular galaxies}},
	Volume = 366,
