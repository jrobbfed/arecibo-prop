	Arxivurl = {http://arXiv.org/abs/astro-ph/0703533},
	Author = {{Cappellari}, M. and {Emsellem}, E. and {Bacon}, R. and {Bureau}, M. and {Davies}, R.~L. and {de Zeeuw}, P.~T. and {Falc{\'o}n-Barroso}, J. and {Krajnovi{\'c}}, D. and {Kuntschner}, H. and {McDermid}, R.~M. and {Peletier}, R.~F. and {Sarzi}, M. and {van den Bosch}, R.~C.~E. and {van de Ven}, G.},
