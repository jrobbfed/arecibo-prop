	Date-Modified = {2014-08-27 18:39:40 +0000},
	Doi = {10.1111/j.1365-2966.2012.21446.x},
	Eprint = {1206.1862},
	Journal = {\mnras},
	Keywords = {stars: neutron, pulsars: general, pulsars: individual: PSR J1614-2230, stars: rotation, white dwarfs, X-rays: binaries},
