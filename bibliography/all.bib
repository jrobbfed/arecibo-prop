	Date-Added = {2014-04-23 19:38:06 +0000},
	Date-Modified = {2014-04-23 19:38:06 +0000},
	Doi = {10.1086/157282},
	Journal = {\apj},
	Keywords = {Elliptical Galaxies, Mathematical Models, Stellar Models, Stellar Motions, Stellar Structure, Stellar Systems, Density Distribution, Dynamic Stability, Gravitational Fields, Integral Equations, Linear Programming, Orbital Mechanics},
	Month = aug,
	Pages = {236-247},
	Title = {{A numerical model for a triaxial stellar system in dynamical equilibrium}},
	Volume = 232,
	Year = 1979,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1086/157282},
