	Eprint = {1406.5191},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = jun,
	Title = {{Decoding the Star-Forming Main Sequence or: How I Learned to Stop Worrying and Love the Central Limit Theorem}},
	Year = 2014,
