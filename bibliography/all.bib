	Doi = {10.1111/j.1365-2966.2006.11013.x},
	Eprint = {astro-ph/0512123},
	Journal = {\mnras},
	Keywords = {black hole physics, gravitational waves, galaxies: evolution, dark matter},
	Month = nov,
	Pages = {1540-1548},
	Title = {{The role of primordial kicks on black hole merger rates}},
	Volume = 372,
	Year = 2006,
